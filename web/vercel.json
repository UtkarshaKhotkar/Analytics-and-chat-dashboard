{
  "buildCommand": "cd ../.. && npm run build --filter=@flowbit/web",
  "installCommand": "cd ../.. && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-api-url.vercel.app/api/:path*"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE": "/api"
  }
}



